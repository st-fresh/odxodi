    "babel-preset-react-native": "^2.1.0",
    "eslint": "^4.3.0",
    "eslint-config-fbjs-opensource": "^1.0.0",
    "prettier": "^1.5.3"
  },
  "dependencies": {
    "ctx-polyfill": "lifaon74/ctx-polyfill",
    "parse5": "^3.0.2"
  },
  "repository": "https://github.com/iddan/react-native-canvas"
